\\\Question1
package fudam;

public class swp {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// AS-1
		int fno =100, sno = 200, tsno = 0;
		System.out.println("--- Before Swapping" );		
		System.out.println("Fno" + "\t"+fno );
		System.out.println("Sno" + "\t"+sno );	
		 tsno = fno;
		fno = sno;
		 sno = tsno;	
		System.out.println("--- After Swapping" );
		System.out.println("Fno" + "\t"+fno );
		System.out.println("Sno" + "\t"+sno );
   		

	}

}
\\\Question2
package fudam;

public class prob2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int fno =100, sno = 200;
		System.out.println("--- Before Swapping" + "Fno: " + "\t"+ fno +"Sno" + "\t"+sno );
		int tno = fno + sno; // 300
		int lno = sno - fno; // 100
			fno = tno - lno;//200
			sno = lno;
		System.out.println("--- After Swapping" + "Fno: " + "\t"+ fno +"Sno: " + "\t"+sno );
			
		
		    
 
	}

}

\\\Question 3
package fudam;

public class Prob3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int fno =100, sno = 200, tsno = 300 , ut = 0 ;
		System.out.println("--- Before Swapping" + "Fno: " + "\t"+ fno +"Sno:" + "\t"+ sno +"tno:" + "\t"+ tsno);
		ut = fno ;//ut =100
		fno = sno;//fno=200
		sno = tsno ;//sno =300
		tsno = ut ;//tsno = 100
		System.out.println("--- After Swapping" +"\t"+"Fno : " + "\t"+ fno+"\t"+"Sno : " + "\t"+ sno +"\t"+"tno : " + "\t"+ tsno);

		
		
		
		 
		
			
		

	}

}
\\\Question 4
package fudam;

public class prob4 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int fno =100, sno = 200, tsno = 300;
		System.out.println("--- Before Swapping" + "Fno: " + "\t"+ fno +"Sno:" + "\t"+ sno +"tno:" + "\t"+ tsno);
		int asno = fno + sno + tsno;//asno= 600;
		int bsno = fno - sno - tsno;//bsno= -400;
		int dsno = tsno - sno;// 100
		tsno = asno + bsno; //200
		sno = dsno; // 100
		fno = asno + bsno + dsno;// 300
		System.out.println("--- After Swapping" + "Fno: " + "\t"+ fno +"Sno:" + "\t"+ sno +"tno:" + "\t"+ tsno);
	
		
	}

}
\\\Question 5 
package fudam;

public class prob5 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int fno =100, sno = 200, tsno = 300, fsno =400 ,ano = 0 ;
		System.out.println("--- Before Swapping" + "Fno: " + "\t"+ fno +"Sno:" + "\t"+ sno +"tno:" + "\t"+ fsno+"fsno:" + "\t"+ fsno);
		ano = fno;//100
		fno = sno;//200
		sno = tsno;//300
		tsno = fsno;//400
		fsno = ano;
		System.out.println("--- After Swapping" + "Fno: " + "\t"+ fno +"Sno:" + "\t"+ sno +"tno:" + "\t"+ tsno+"fsno:" + "\t"+ fsno);
		
		
	
		
		
		
		

	}

}
\\\Question 6
package fudam;

public class prob6 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int fno =100, sno = 200, tsno = 300, fsno =400  ;
		System.out.println("--- Before Swapping" + "Fno: " + "\t"+ fno +"Sno:" + "\t"+ sno +"tno:" + "\t"+ fsno+"fsno:" + "\t"+ tsno);
		int ano = fno + sno + tsno + fsno ;//1000
		int bno = fno - sno - tsno - fsno ;//-800
		int cno = fno + sno - tsno - fsno ;//-400
		int dno = fno + sno ;//300
		int eno = fno + tsno ;
		
		fno = ano + bno ;//200
		sno = dno;//300
		tsno =fsno ;//400
		fsno = tsno - sno;//100
		System.out.println("--- After Swapping" + "Fno: " + "\t"+ fno +"Sno:" + "\t"+ sno +"tno:" + "\t"+ tsno+"fsno:" + "\t"+ fsno);

		
	}

}
