\\\Question 10
package fudam;

public class prob10 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int pri = 500000;
		int tim = 5 ; 
		double rat = 0.068d ;
		double int1 = pri * tim * rat ;
		double int2 = int1 / 100 ;
		double amt = int2 + pri ;
		System.out.println("total interest amount " + "\t" + int2 + "total deposit amount" +"\t"+ amt);
		
		

	}

}
\\\Question 11
package fudam;

public class prob11 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int bs = 85000;
		double ta = 0.15d;
		double da = 0.2d;
		double hra = 0.18*bs ;
		double pf = 0.2d ; 
		double tax = 0.25*bs ;
		double gross = bs + ta + da + hra ;
		double net = gross -  (pf +tax);
		System.out.println("total gross amount " + "\t" + gross + "total net amount" +"\t"+ net);	

	}

}

