public class prob7 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int spc = 699;
		float gst = 0.18f;
		float tgst = spc*gst;
		double tcost = spc + tgst ;
		System.out.println("total Plan Cost " + "\t" + tcost); 
		
				

	}

}
\\\Question 8
package fudam;

public class prob8 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		float pizza1 = 399.0f;
		float pizza = 2*pizza1 ;
		float disc = 0.2f ;
		double  totald = pizza * disc ;
		double totalbill = pizza - totald ;
		System.out.println("total bill Cost " + "\t" + totalbill ); 
		
		
	}

}
